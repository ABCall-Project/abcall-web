{
  "name": "abcall-web",
  "version": "0.0.0",
  "scripts": {
    "setenv": "node ./scripts/set-env.js",
    "ng": "ng",
    "start": "npm run setenv && ng serve",
    "build": "npm run setenv && ng build --configuration production",
    "watch": "npm run setenv && ng build --watch --configuration development",
    "test": "NODE_ENV=test npm run setenv && ng test",
    "test:coverage": "NODE_ENV=test npm run setenv && ng test --code-coverage --karma-config ./karma.conf.js",
    "test:ci": "NODE_ENV=test npm run setenv && ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:coverage:ci": "NODE_ENV=test npm run setenv && ng test --code-coverage --karma-config ./karma.conf.js --no-watch --no-progress --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.1",
    "@angular/cdk": "^16.0.1",
    "@angular/common": "^16.0.1",
    "@angular/compiler": "^16.0.1",
    "@angular/core": "^16.0.1",
    "@angular/forms": "^16.0.1",
    "@angular/material": "^16.0.1",
    "@angular/platform-browser": "^16.0.1",
    "@angular/platform-browser-dynamic": "^16.0.1",
    "@angular/router": "^16.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-tabler-icons": "^2.7.0",
    "chart.js": "^4.4.4",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "ng2-charts": "^6.0.1",
    "ngx-scrollbar": "^11.0.0",
    "rxjs": "~7.5.0",
    "sass": "^1.57.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.1",
    "@angular/cli": "~16.0.1",
    "@angular/compiler-cli": "^16.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/date-fns": "^2.6.0",
    "@types/jasmine": "~4.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.4"
  }
}
